fib1 = 1
fib2 = 1

n = input("Номер элемента ряда Фибоначчи: ")
n = int(n) #вводится номер элемента ряда Фибоначчи

i = 0 #номер счётчика для работы цикла
while i < n - 2: #условие цикла
    fib_sum = fib1 + fib2 #
    fib1 = fib2 # равенство 2х чисел
    fib2 = fib_sum
    i = i + 1 #сделать шаг вперёд

print(fib2) #вывести результат числа
